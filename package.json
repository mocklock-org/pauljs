{
  "name": "pauljs",
  "version": "1.11.2-alpha.0",
  "description": "A lightweight framework for building fast landing pages",
  "main": "dist/index.js",
  "bin": {
    "pauljs": "bin/cli.js",
    "create-pauljs-app": "bin/create.js"
  },
  "type": "commonjs",
  "scripts": {
    "start": "node bin/cli.js serve",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:dev": "cross-env NODE_ENV=development node scripts/build.js",
    "dev": "cross-env NODE_ENV=development nodemon bin/cli.js serve",
    "prepare": "npm run build",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "analyze": "source-map-explorer dist/**/*.js",
    "benchmark": "node scripts/benchmark.js"
  },
  "files": [
    "dist",
    "bin",
    "scripts",
    "README.md",
    "LICENSE",
    "template"
  ],
  "keywords": [
    "landing-page",
    "framework",
    "static-site",
    "javascript",
    "template-engine",
    "create-pauljs-app",
    "pauljs"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocklock-org/pauljs.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@babel/register": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "csso": "^5.0.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "inquirer": "^8.2.6",
    "pauljs": "^1.10.4-alpha.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.15",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.22.15",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.3.23",
    "benchmark": "^2.1.4",
    "cross-env": "^7.0.3",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "source-map-explorer": "^2.5.3",
    "standard-version": "^9.5.0",
    "terser": "^5.19.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
