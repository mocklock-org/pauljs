{
  "name": "pauljs",
  "version": "1.11.2-alpha.3",
  "description": "A lightweight framework for building fast landing pages",
  "main": "dist/index.mjs",
  "type": "module",
  "bin": {
    "pauljs": "bin/cli.mjs",
    "create-pauljs-app": "bin/create.mjs"
  },
  "scripts": {
    "build": "tsc && cross-env NODE_OPTIONS=\"--loader ts-node/esm\" node scripts/build.mts",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint . --ext .ts,.mts",
    "format": "prettier --write \"src/**/*.{ts,mts}\" \"scripts/**/*.{ts,mts}\" \"bin/**/*.{ts,mts}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-object-rest-spread": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@babel/register": "^7.24.0",
    "autoprefixer": "^10.4.18",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "csso": "^5.0.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "inquirer": "^9.2.15",
    "postcss": "^8.4.35",
    "sass": "^1.71.1",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "terser": "^5.28.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/benchmark": "^2.1.5",
    "@types/csso": "^5.0.4",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "bin",
    "scripts",
    "template",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "landing-page",
    "framework",
    "static-site",
    "react",
    "typescript",
    "esm"
  ],
  "author": "MockLock Organization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocklock-org/pauljs.git"
  },
  "bugs": {
    "url": "https://github.com/mocklock-org/pauljs/issues"
  },
  "homepage": "https://github.com/mocklock-org/pauljs#readme"
}
